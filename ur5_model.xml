<?xml version="1.0" ?>
<mujoco model="ur5">
  <compiler angle="radian" meshdir="meshes"/>
  
  <option timestep="0.002" gravity="0 0 -9.81">
    <flag warmstart="enable"/>
  </option>

  <visual>
    <quality shadowsize="4096"/>
    <map znear="0.01"/>
  </visual>

  <asset>
    <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="512"/>
    <texture name="texplane" type="2d" builtin="checker" rgb1=".2 .3 .4" rgb2=".1 0.15 0.2" width="512" height="512" mark="cross" markrgb=".8 .8 .8"/>
    <material name="matplane" reflectance="0.3" texture="texplane" texrepeat="1 1" texuniform="true"/>
  </asset>

  <worldbody>
    <light directional="true" diffuse=".8 .8 .8" specular="0.3 0.3 0.3" pos="0 0 5.0" dir="0 0 -1" castshadow="false"/>
    <light directional="true" diffuse=".6 .6 .6" specular="0.2 0.2 0.2" pos="0 0 4" dir="0 0 -1"/>
    <geom name="floor" pos="0 0 0" size="0 0 0.05" type="plane" material="matplane" condim="3"/>

    <!-- Base -->
    <body name="base" pos="0 0 0.1">
      <inertial pos="0 0 0" mass="4.0" diaginertia="0.00443333156 0.00443333156 0.0072"/>
      <geom type="cylinder" size="0.06 0.05" rgba="0.7 0.7 0.7 1" mass="4.0"/>
      
      <!-- Shoulder Link -->
      <body name="shoulder_link" pos="0 0 0.08916">
        <inertial pos="0 -0.00193 -0.02651" mass="3.7" diaginertia="0.0084 0.0084 0.0064"/>
        <joint name="shoulder_pan_joint" type="hinge" axis="0 0 1" limited="true" range="-6.2832 6.2832" damping="0.5"/>
        <geom type="cylinder" size="0.06 0.04" rgba="0.0 0.0 0.8 1"/>
        
        <!-- Upper Arm Link -->
        <body name="upper_arm_link" pos="0 0.13585 0" quat="0.707107 0 0.707107 0">
          <inertial pos="0.2125 0 0.11336" mass="8.393" diaginertia="0.0462 0.0462 0.0021"/>
          <joint name="shoulder_lift_joint" type="hinge" axis="0 0 1" limited="true" range="-6.2832 6.2832" damping="0.5"/>
          <geom type="capsule" fromto="0 0 0 0.425 0 0" size="0.045" rgba="0.8 0.0 0.0 1"/>
          
          <!-- Forearm Link -->
          <body name="forearm_link" pos="0.425 0 0">
            <inertial pos="0.27232 0 0.0265" mass="2.275" diaginertia="0.0462 0.0462 0.0016"/>
            <joint name="elbow_joint" type="hinge" axis="0 0 1" limited="true" range="-3.1416 3.1416" damping="0.5"/>
            <geom type="capsule" fromto="0 0 0 0.39225 0 0" size="0.04" rgba="0.0 0.8 0.0 1"/>
            
            <!-- Wrist 1 Link -->
            <body name="wrist_1_link" pos="0.39225 0 0.10915" quat="0.707107 0 -0.707107 0">
              <inertial pos="0 0.01634 -0.03410735" mass="1.219" diaginertia="0.0016 0.0016 0.0009"/>
              <joint name="wrist_1_joint" type="hinge" axis="0 0 1" limited="true" range="-6.2832 6.2832" damping="0.1"/>
              <geom type="cylinder" size="0.03 0.05" rgba="0.0 0.0 0.8 1"/>
              
              <!-- Wrist 2 Link -->
              <body name="wrist_2_link" pos="0 -0.09456 0" quat="0.707107 0 0.707107 0">
                <inertial pos="0 -0.01634 -0.0018" mass="1.219" diaginertia="0.0016 0.0016 0.0009"/>
                <joint name="wrist_2_joint" type="hinge" axis="0 0 1" limited="true" range="-6.2832 6.2832" damping="0.1"/>
                <geom type="cylinder" size="0.03 0.04" rgba="0.8 0.0 0.0 1"/>
                
                <!-- Wrist 3 Link -->
                <body name="wrist_3_link" pos="0 0 0.0823">
                  <inertial pos="0 0 -0.001159" mass="0.1879" diaginertia="0.0001 0.0001 0.0001"/>
                  <joint name="wrist_3_joint" type="hinge" axis="0 0 1" limited="true" range="-6.2832 6.2832" damping="0.1"/>
                  <geom type="cylinder" size="0.025 0.02" rgba="0.0 0.8 0.0 1"/>
                  
                  <!-- End Effector -->
                  <site name="ee_site" pos="0 0 0.05" size="0.01" rgba="1 0 0 1"/>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <actuator>
    <motor name="shoulder_pan" joint="shoulder_pan_joint" gear="100" ctrllimited="true" ctrlrange="-150 150"/>
    <motor name="shoulder_lift" joint="shoulder_lift_joint" gear="100" ctrllimited="true" ctrlrange="-150 150"/>
    <motor name="elbow" joint="elbow_joint" gear="100" ctrllimited="true" ctrlrange="-150 150"/>
    <motor name="wrist_1" joint="wrist_1_joint" gear="10" ctrllimited="true" ctrlrange="-28 28"/>
    <motor name="wrist_2" joint="wrist_2_joint" gear="10" ctrllimited="true" ctrlrange="-28 28"/>
    <motor name="wrist_3" joint="wrist_3_joint" gear="10" ctrllimited="true" ctrlrange="-28 28"/>
  </actuator>

  <sensor>
    <jointpos name="shoulder_pan_pos" joint="shoulder_pan_joint"/>
    <jointpos name="shoulder_lift_pos" joint="shoulder_lift_joint"/>
    <jointpos name="elbow_pos" joint="elbow_joint"/>
    <jointpos name="wrist_1_pos" joint="wrist_1_joint"/>
    <jointpos name="wrist_2_pos" joint="wrist_2_joint"/>
    <jointpos name="wrist_3_pos" joint="wrist_3_joint"/>
    
    <jointvel name="shoulder_pan_vel" joint="shoulder_pan_joint"/>
    <jointvel name="shoulder_lift_vel" joint="shoulder_lift_joint"/>
    <jointvel name="elbow_vel" joint="elbow_joint"/>
    <jointvel name="wrist_1_vel" joint="wrist_1_joint"/>
    <jointvel name="wrist_2_vel" joint="wrist_2_joint"/>
    <jointvel name="wrist_3_vel" joint="wrist_3_joint"/>
    
    <framepos name="ee_pos" objtype="site" objname="ee_site"/>
    <framequat name="ee_quat" objtype="site" objname="ee_site"/>
  </sensor>
</mujoco>
